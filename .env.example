OPENAI_API_KEY=your-openai-api-key
HUGGINGFACE_API_KEY=

# Runtime environment: development | staging | production
ENVIRONMENT=development
LOG_LEVEL=INFO

# Model settings
MODEL_NAME=gpt-4o-mini
FAST_MODEL_NAME=gpt-4o-mini
STRONG_MODEL_NAME=gpt-4o
AUTO_MODEL_ROUTING=true
TOOL_MANDATORY_FOR_HIGH_RISK=true
TEMPERATURE=0.3
MAX_TOKENS=2000

# Security: keep disabled for public deployments
ENABLE_SANDBOX=false

# This project treats .env as the single source of truth for app configuration.

# Optional local path overrides
# DATA_DIR=./data
# LOGS_DIR=./data/logs
# SESSIONS_DIR=./data/sessions
